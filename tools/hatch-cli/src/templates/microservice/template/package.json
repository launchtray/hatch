{
  "name": "@launchtray/hatch-template-microservice",
  "version": "0.1.0",
  "license": "MIT",
  "private": true,
  "publishConfig": {
    "access": "restricted"
  },
  "scripts": {
    "start": "heft start",
    "lint": "eslint --max-warnings=0 .",
    "lint:fix": "eslint --max-warnings=0 --fix .",
    "build": "heft build --clean",
    "test": "jest --passWithNoTests --verbose --runInBand --detectOpenHandles $JEST_CLI_OPTIONS 2>&1",
    "start:prod": "NODE_ENV=production node build/server.js"
  },
  "dependencies": {
    "@launchtray/hatch-server": "workspace:*",
    "@launchtray/hatch-util": "workspace:*",
    "@launchtray/hatch-server-middleware": "workspace:*",
    "@launchtray/tsyringe-async": "4.3.3",
    "cors": "2.8.5",
    "express": "^4",
    "invariant": "^2",
    "winston": "^3",
    "winston-transport": "^4",
    "@sentry/node": "^5",
    "@sentry/browser": "^5",
    "ws": "^7",
    "bufferutil": "^4",
    "utf-8-validate": "^5",
    "swagger-ui": "4.5.2",
    "isarray": "^2.0.5",
    "regenerator-runtime": "^0.13.3",
    "supports-color": "^7.2.0",
    "debug": "^4.3.1",
    "formdata-node": "^6.0.3",
    "process": "^0.11.10",
    "buffer": "^6.0.3",
    "console-browserify": "^1.2.0",
